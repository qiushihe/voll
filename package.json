{
  "name": "voll",
  "version": "1.7.3",
  "description": "Voll",
  "repository": "https://github.com/qiushihe/voll",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean": "shx rm -fr ./dist/* ./build/*",
    "build-dev": "npm run clean && webpack --mode development",
    "build-prod": "npm run clean && webpack -p --mode production",
    "build-icons": "node ./scripts/rebuild-icons.js",
    "build-package-json": "node ./scripts/rebuild-package-json.js",
    "start": "npm run build-dev && npm run build-package-json && npm run build-icons && electron ./build",
    "package": "npm run build-prod && npm run build-package-json && npm run build-icons && run-script-os",
    "package:linux": "electron-packager ./build --icon=./build/lolgo-512.png --out=./dist",
    "package:darwin": "electron-packager ./build --icon=./build/lolgo.icns --out=./dist",
    "package:win32": "electron-packager ./build --icon=./build/lolgo.ico --out=./dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-provide-modules": "^1.0.3",
    "babel-plugin-styled-components": "^1.10.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.6",
    "electron-dl": "^1.13.0",
    "electron-packager": "^13.0.0",
    "file-loader": "^3.0.1",
    "graceful-fs": "^4.1.15",
    "html-webpack-plugin": "^3.2.0",
    "icon-gen": "^2.0.0",
    "jimp": "^0.6.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "raw-loader": "^1.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.2",
    "run-script-os": "^1.0.5",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "simple-spellchecker": "^0.9.7",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2",
    "typeface-roboto": "0.0.54",
    "url-loader": "^1.1.2",
    "useragent-generator": "^1.1.0",
    "uuid": "^3.3.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
